(defwidget time [] 
(box :halign "center" 
     :class "time" 
     :space-evenly "false"
(label 
    :halign "center"
    :text "${hour}  |  ")
(label 
    :halign "center"
    :text "${minute}  |  ")
(label 
    :halign "center"
    :text "${second}  ")))

(defwidget rquo []
  (box :halign "center"
       :class "quo"
    (label :halign "center" :class "qt"
      :text "${rick}")))
